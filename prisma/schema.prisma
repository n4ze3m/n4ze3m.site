generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Projects {
  id          Int      @id @default(autoincrement())
  name        String
  github_url  String
  demo_url    String?
  description String   @db.LongText
  created_at  DateTime @default(now())
  updated_at  DateTime @updatedAt
}
